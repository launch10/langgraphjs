{
  "compilerOptions": {
    "target": "ES2022",
    "useDefineForClassFields": true,
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "strict": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "baseUrl": ".",
    "paths": {
      "@langchain/langgraph-sdk": ["../../libs/sdk/src/index.ts"],
      "@langchain/langgraph-sdk/*": ["../../libs/sdk/src/*.ts"],
      "@langchain/langgraph-api": ["../../libs/langgraph-api/src/cli/spawn.mts"],
      "@langchain/langgraph-api/*": ["../../libs/langgraph-api/src/*.mts"],
      "@langchain/langgraph": ["../../libs/langgraph/src/index.ts"],
      "@langchain/langgraph/*": ["../../libs/langgraph/src/*.ts"]
    }
  },
  "include": ["src"],
  "ts-node": {
    "esm": true
  }
}
